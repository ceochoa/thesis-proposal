@inproceedings{2012-FOCS-IntegerSrtingInNSqrtLgLgNExpectedTimeAndLinearSpace-HanThorup,
author={Han, Y. and Thorup, M.},
booktitle=focs,
title={Integer sorting in {$O(n\sqrt{\log\log n})$} expected time and linear space},
