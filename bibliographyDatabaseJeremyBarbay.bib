@article{1982-IPL-AnIntroductionToThreeAlgorithmsForSortingInSitu-DijkstarVanGasteren,
  author =       "E. W. Dijkstra and A.J.M. van Gasteren",
  title =        "An introduction to three algorithms for sorting in
                  situ",
  journal =      "Information Processing Letters",
  volume =       "15(3)",
  pages =        "129-134",
  year =         1982,
  abstract =      {Let $N$ be a planar undirected network with
                  distinguished vertices $s$, $t$, a total of $n$
                  vertices, and each edge labeled with a positive real
                  (the edge's cost) from a set $L$. This paper
                  presents an algorithm for computing a minimum (cost)
                  $s-t$ cut of $N$. For general $L$, this algorithm
                  runs in time $O(n \log_2 n)$ time on a (uniform cost
                  criteria) RAM. For the case $L$ contains only
                  integers smaller than $n$, the algorithm runs in
                  time $O(n \log(n)\log\log(n))$. Our algorithm also
                  constructs a minimum $s-t$ cut of a planar graph
                  (i.e., for the case $L= \{1\}$) in time $O(n
                  \log(n))$.},
}

